[project]
	description = Device Tree for Samsung Galaxy Note 3 SM-N900
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/halaszk
[access "refs/for/refs/*"]
	read = group user/halaszk
	push = group user/halaszk
	label-Code-Review = -2..+2 group user/halaszk
	label-Verified = -1..+1 group user/halaszk
	rebase = group user/halaszk
	removeReviewer = group user/halaszk
	submit = group user/halaszk
[access "refs/heads/*"]
	read = group user/halaszk
	create = group user/halaszk
	push = +force group user/halaszk
	pushMerge = group user/halaszk
[access "refs/tags/*"]
	push = +force group user/halaszk
	pushTag = group user/halaszk
	pushSignedTag = group user/halaszk
